        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
        <StyleInclude Source="avares://Wabbajack.App/Assets/Wabbajack.axaml" />

    </Application.Styles>
</Application>